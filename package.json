{
  "name": "jwt-redux-devtools",
  "version": "1.0.9",
  "description": "Redux middleware to develop JWT-based apps",
  "main": "./dist/index.js",
  "repository": "git@github.com:hex22a/jwt-redux-devtools.git",
  "author": "hex22a <hex22a@gmail.com>",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/__mocks__/localStorageMock.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "scripts": {
    "start": "yarn build",
    "build:dev": "NODE_ENV=development webpack",
    "build": "NODE_ENV=production webpack -p",
    "test": "NODE_ENV=test jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:watch": "yarn test -- --watch"
  },
  "files": [
    "LICENSE",
    "dist/",
    "README.md"
  ],
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.18.0",
    "eslint-config-fs": "^0.1.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^20.0.1",
    "webpack": "^2.3.2"
  },
  "dependencies": {
    "react": "^15.5.4",
    "redux": "^3.6.0"
  }
}
